{
    "name": "lakm/laravel-comments-admin-panel",
    "description": "Integrate seamless commenting functionality into your Laravel project.",
    "keywords": [
        "laravel",
        "comments",
        "Admin",
        "Livewire",
        "Alpine.js"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lakshan Madushanka",
            "email": "epmadushanka@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "lakm/laravel-comments": "^1.0|^2.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "filament/notifications": "^3.2",
        "filament/tables": "^3.2"
    },
    "require-dev": {
        "larastan/larastan": "2.*",
        "orchestra/testbench": "8.*",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3",
        "pestphp/pest-plugin-livewire": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LakM\\CommentsAdminPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LakM\\CommentsAdminPanel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LakM\\CommentsAdminPanel\\CommentsAdminPanelServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyze": "./vendor/bin/phpstan analyse"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
	"prefer-stable": true,
    "minimum-stability": "stable"
}
